{
  "name": "docs",
  "version": "6.0.0",
  "private": true,
  "engines": {
    "node": ">=20 <=24"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build --turbo",
    "start": "next start",
    "clean": "rm -rf .source .next tsconfig.tsbuildinfo",
    "lint": "biome check . && prettier . --check",
    "lint:fix": "biome check . --write && prettier . --write",
    "typecheck": "tsc --pretty --noEmit",
    "postinstall": "fumadocs-mdx"
  },
  "dependencies": {
    "fumadocs-core": "15.6.10",
    "fumadocs-mdx": "11.7.5",
    "fumadocs-ui": "15.6.10",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2"
  }
}
